    "\n",
    "costs_gd=[]\n",
    "b=0\n",
    "w_gd = np.zeros(len(x[0])) \n",
    "# #train GD\n",
    "for i in range(repeat):\n",
    "    y_pred=np.dot(x,w_gd)+b \n",
    "    \n",
    "    cost=(1/m)*np.sum(((y_pred-y)**2))\n",
    "    costs_gd.append(cost)\n",
    "    d_w=(2/m)*np.dot(x.T,(y_pred-y))\n",
    "    d_b=(1/m)*np.sum(y_pred-y)\n",
    "    w_gd=w_gd-l_rate*d_w\n",
    "    b=b-l_rate*d_b\n",
